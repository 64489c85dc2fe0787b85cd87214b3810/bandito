{
  "name": "@mftd03/bandito",
  "version": "0.0.9",
  "scripts": {
    "dev": "vite dev",
    "docs:build": "vite build",
    "docs:preview": "vite preview",
    "package:build": "svelte-kit sync && svelte-package && publint",
    "package:release": "standard-version && git push --follow-tags && npm publish",
    "build:wc": "vite build -c vite.wc.js",
    "ci:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ci:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "ci:lint": "prettier --check .",
    "ci:format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0-next.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "sass": "^1.77.4",
    "standard-version": "^9.5.0",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mftd03/bandito"
  },
  "publishConfig": {
    "@mftd03:registry": "https://npm.pkg.github.com"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
